{
   "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
   "version": 1,
   "newProjectRoot": "projects",
   "projects": {
      "accountui": {
         "projectType": "application",
         "schematics": {
         },
         "root": "",
         "sourceRoot": "src",
         "prefix": "app",
         "architect": {
            "build": {
               "builder": "@angular-devkit/build-angular:application",
               "options": {
                  "optimization": false,
                  "extractLicenses": false,
                  "sourceMap": true,
                  "namedChunks": true,                  
                  "outputPath": {
                     "base": "dist/accountui"
                  },
                  "index": "src/index.html",
                  "polyfills": [
                     "zone.js",
                     "@angular/localize/init"
                  ],
                  "allowedCommonJsDependencies": [ "hammerjs" ],
                  "tsConfig": "tsconfig.app.json",
                  "assets": [
                     "src/favicon.ico",
                     "src/assets"
                  ],
                  "styles": [
                     "src/sass/custom-theme.scss",
                     "src/styles.css",
                     "node_modules/bootstrap/dist/css/bootstrap.min.css",
                     "node_modules/bootstrap-icons/font/bootstrap-icons.css"
                  ],
                  "stylePreprocessorOptions": {
                     "includePaths": [
                     "src/sass"
                     ],
                     "sass": {
                        "silenceDeprecations": ["mixed-decls", "color-functions", "global-builtin", "import"]
                      }
                  },
                  "scripts": [
                  ],
                  "browser": "src/main.ts"
               },
               "configurations": {
                  "production": {
                     "budgets": [
                        {
                           "type": "initial",
                           "maximumWarning": "3200kb",
                           "maximumError": "6mb"
                        },
                        {
                           "type": "anyComponentStyle",
                           "maximumWarning": "4mb",
                           "maximumError": "6mb"
                        }
                     ],
                     "fileReplacements": [
                        {
                           "replace": "src/environments/environment.ts",
                           "with": "src/environments/environment.prod.ts"
                        }
                     ],
                     "outputHashing": "all",
                     "optimization": {
                        "scripts": true,
                        "styles": {
                           "minify": true,
                           "inlineCritical": false
                        },
                        "fonts": true
                     }
                  },
                  "development": {

                  },
                  "nobe": {
                     "fileReplacements": [
                        {
                           "replace": "src/environments/environment.ts",
                           "with": "src/environments/environment.nobe.ts"
                        }
                     ]                     
                  },
                  "nobegh": {
                     "fileReplacements": [
                        {
                           "replace": "src/environments/environment.ts",
                           "with": "src/environments/environment.nobegh.ts"
                        }
                     ]                     
                  }
               },
               "defaultConfiguration": "production"
            },
            "serve": {
               "builder": "@angular-devkit/build-angular:dev-server",
               "configurations": {
                  "production": {
                     "buildTarget": "accountui:build:production"
                  },
                  "development": {
                     "buildTarget": "accountui:build:development"
                  },
                  "nobe": {
                     "buildTarget": "accountui:build:nobe"
                  },
                  "nobegh": {
                     "buildTarget": "accountui:build:nobegh"
                  }
               },
               "defaultConfiguration": "development"
            },
            "extract-i18n": {
               "builder": "@angular-devkit/build-angular:extract-i18n",
               "options": {
                  "buildTarget": "accountui:build"
               }
            },
            "test": {
               "builder": "@angular-devkit/build-angular:karma",
               "options": {
                  "main": "src/test.ts",
                  "polyfills": [
                     "src/polyfills.ts",
                     "@angular/localize/init"
                  ],
                  "tsConfig": "tsconfig.spec.json",
                  "karmaConfig": "karma.conf.js",
                  "assets": [
                     "src/favicon.ico",
                     "src/assets"
                  ],
                  "styles": [
                     "src/styles.css"
                  ],
                  "scripts": [
                  ]
               }
            }
         }
      }
   },
   "cli": {
      "analytics": false
   }
}
